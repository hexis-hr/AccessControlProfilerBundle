{
  "name": "hexis-hr/access-control-profiler-bundle",
  "type": "symfony-bundle",
  "description": "Access Control Profiler Bundle",
  "keywords": [
    "php",
    "symfony",
    "reusable bundle"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Hexis",
      "homepage": "https://hexis.hr/"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Hexis\\AccessControlProfilerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hexis\\AccessControlProfilerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/dependency-injection": "^v6.0.0",
    "symfony/config": "^v6.0.0"
  },
  "require-dev": {
    "symfony/http-kernel": "^v6.0.0",
    "phpunit/phpunit": "^10.5",
    "symfony/web-profiler-bundle": "^v6.0.0"
  }
}
